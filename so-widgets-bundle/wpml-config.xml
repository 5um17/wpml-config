<wpml-config>
  <admin-texts>
    <key name="widget_sow-button">
      <key name="*">
        <key name="text" />
        <key name="url" />
        <key name="attributes">
          <key name="title" />
        </key>
      </key>
    </key>
    <key name="widget_sow-editor">
      <key name="*">
        <key name="title" />
        <key name="text" />
      </key>
    </key>
    <key name="widget_sow-contact-form">
      <key name="*">
        <key name="title" />
        <key name="settings">
          <key name="default_subject" />
          <key name="subject_prefix" />
          <key name="success_message" />
          <key name="submit_text" />
        </key>
        <key name="fields">
          <key name="*">
            <key name="label" />
            <key name="description" />
            <key name="required">
              <key name="missing_message" />
            </key>
          </key>
        </key>
      </key>
    </key>
    <key name="widget_sow-headline">
      <key name="*">
        <key name="headline">
          <key name="text" />
          <key name="destination_url" />
        </key>
        <key name="sub_headline">
          <key name="text" />
          <key name="destination_url" />
        </key>
      </key>
    </key>
    <key name="widget_sow-hero">
      <key name="*">
        <key name="frames">
          <key name="*">
            <key name="content" />
          </key>
        </key>
      </key>
    </key>
    <key name="widget_sow-image">
      <key name="*">
        <key name="title" />
        <key name="alt" />
        <key name="url" />
      </key>
    </key>
    <key name="widget_sow-features">
      <key name="*">
        <key name="features">
          <key name="*">
            <key name="title" />
            <key name="text" />
            <key name="icon_title" />
            <key name="more_text" />
            <key name="icon_url" />
          </key>
        </key>
      </key>
    </key>
    <key name="widget_sow-post-carousel">
      <key name="*">
        <key name="title" />
      </key>
    </key>
    <key name="widget_sow-testimonials">
      <key name="*">
        <key name="title" />
        <key name="testimonials">
          <key name="*">
            <key name="text" />
            <key name="url" />
          </key>
        </key>
      </key>
    </key>
  </admin-texts>
</wpml-config>
