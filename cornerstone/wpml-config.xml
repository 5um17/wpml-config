                <attribute>title</attribute>
            </attributes>
        </shortcode>
        <shortcode>
            <tag>x_blockquote</tag>
        </shortcode>
        <shortcode>
            <tag>x_button</tag>
            <attributes>
                <attribute type="link">href</attribute>
                <attribute>title</attribute>
                <attribute>info_content</attribute>
            </attributes>
        </shortcode>
        <shortcode>
            <tag>x_callout</tag>
            <attributes>
                <attribute>title</attribute>
                <attribute>message</attribute>
                <attribute>button_text</attribute>
                <attribute type="link">href</attribute>
